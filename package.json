  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DeveloperRejaul/react-simple-query/issues"
  },
  "keywords": [
    "javascript",
    "react-simple-query",
    "react",
    "react-native"
  ],
  "homepage": "https://github.com/DeveloperRejaul/react-simple-query#readme",
  "files": [
    "dist/**/*",
    "README.md"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "devDependencies": {
    "@types/react": "^19.1.16",
    "react": "^19.1.1",
    "@types/node": "^22.7.4",
    "ts-node": "^10.9.2",
