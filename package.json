{
  "name": "react-simple-query",
  "version": "1.0.6",
  "description": "",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --clean --external react",
    "start:cjs": "node dist/cjs/index.js",
    "start:esm": "node dist/esm/index.js",
    "start": "ts-node src/index.ts",
    "test": "npm run build && npm link"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "directories": {
    "dist": "dist`"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DeveloperRejaul/react-simple-query.git"
  },
